{
  "model_name": "Wikidepia/IndoT5-base",
  "use_gpu": true,
  "max_length": 512,
  "num_beams": 4,
  "temperature": 0.8,
  "top_k": 50,
  "top_p": 0.9,
  "repetition_penalty": 1.1,
  "min_quality_threshold": 60.0,
  "neural_confidence_threshold": 0.7,
  "semantic_similarity_threshold": 0.8,
  "synonym_replacement_rate": 0.3,
  "min_synonym_confidence": 0.6,
  "max_synonyms_per_word": 3,
  "preserve_entities": true,
  "context_aware_synonyms": true,
  "max_transformations_per_sentence": 2,
  "transformation_types": [
    "active_to_passive",
    "clause_reordering",
    "conjunction_substitution",
    "modifier_adjustment"
  ],
  "lexical_diversity_weight": 0.25,
  "semantic_preservation_weight": 0.35,
  "syntactic_complexity_weight": 0.2,
  "fluency_weight": 0.2,
  "max_processing_time": 30,
  "max_paraphrase_attempts": 3,
  "enable_batch_processing": true,
  "max_batch_size": 10,
  "synonym_file": "sinonim_extended.json",
  "transformation_rules_file": "transformation_rules.json",
  "stopwords_file": "stopwords_id.txt",
  "log_level": "INFO",
  "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
  "log_file": "indot5_hybrid.log"
}